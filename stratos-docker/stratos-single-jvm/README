
container_id=$(sudo docker run -d \
-e EC2_ENABLED=true \
-e EC2_IDENTITY="" \
-e EC2_CREDENTIAL="" \
-e EC2_OWNER_ID="" \
-e EC2_AVAILABILITY_ZONE="" \
-e EC2_SECURITY_GROUPS="" \
-e EC2_KEYPAIR="" \
-e MB_HOSTNAME=10.10.10.10 \
-e MB_PORT=61616 \
apachestratos/stratos-single-jvm)

sudo docker logs -f $container_id

